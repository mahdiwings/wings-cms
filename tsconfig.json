{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "noEmit": false,
     "noEmitOnError": false,
     "isolatedModules": true,
     "strict": false,
     "noImplicitAny": false,
     "noImplicitThis": false,
     "alwaysStrict": false,
     "skipLibCheck": true,
     "noUnusedLocals": false,
    "noUnusedParameters": false,
     "strictNullChecks": false,
    // "skipLibCheck": true,
    // "strict": false,
    // "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    // "isolatedModules": true,
    // "noImplicitAny": false,
    // "noEmitOnError": false,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
